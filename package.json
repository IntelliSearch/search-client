{
  "name": "search-client",
  "version": "0.1.0",
  "private": false,
  "description": "Javascript library for executing searches in the IntelliSearch search-index.",
  "main": "index.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intellisearch/search-client.git"
  },
  "keywords": [
    "javascript",
    "library",
    "SearchService",
    "client",
    "search",
    "IntelliSearch"
  ],
  "author": "Ronny Hanssen <ronny@intellisearch.com> (http://intellisearch.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/intellisearch/search-client/issues"
  },
  "homepage": "https://github.com/intellisearch/search-client#readme",
  "scripts": {
    "clean": "DEL /Q *.d.ts index.js index.js.map >nul 2>&1",
    "prePublishOnly": "npm run clean && cross-env NODE_ENV=production webpack && tsc",
    "json-server": "json-server --watch test-data.json --routes test-data-routes.json",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --hot",
    "pretest": "cross-env NODE_ENV=ci webpack",
    "test": "cat public/index.html | browser-run --input html --static ./public | tap-spec"
  },
  "dependencies": {
    "@types/deepmerge": "^1.3.0",
    "@types/whatwg-fetch": "^0.0.33",
    "deepmerge": "^1.3.1",
    "domain-task": "^2.0.2"
  },
  "devDependencies": {
    "@types/tape": "^4.2.28",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-discard-module-references": "^1.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "browser-run": "^3.1.0",
    "browser-tap": "^0.0.1",
    "cross-env": "^3.1.4",
    "eslint-config-defaults": "^9.0.0",
    "source-map-loader": "^0.1.6",
    "tap-spec": "^4.1.1",
    "typescript": "^2.1.4",
    "webpack": "^2.2.0-rc.4",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
}

{
    "configuration": {
        "settings": {
            "enabled": false
        },
        "categoryPresentation": {
            "enabled": false
        }
    },
    "match": {
        "pager": {
            "size": 10,
            "addFirst": true,
            "addPrev": true,
            "addNext": true,
            "addLast": true
        },
        "icon": {
            "primary": {
                "property": "sourceName",
                "match": "/(.+)/",
                "replacement": "$1",
                "map": {
                    "": "/img/INTS_Logo-64.svg",
                    "docs.global.ints": "./img/sources/source-fileshare.png",
                    "demo-esp": "./img/sources/source-fileshare.png",
                    "Mailbox": "./img/sources/source-mailbox.png"
                },
                "unresolved": "./img/sources/source-unresolved.png"
            },
            "overlay": {
                "filetype": {
                    "category": "/^FileType.Extension$/i",
                    "metadata": "Extension",
                    "match": "/\\.?(.+)/",
                    "replacement": "$1",
                    "map": {
                        "txt": "./img/filetypes/filetype-txt.png",
                        "doc": "./img/filetypes/filetype-doc.png",
                        "docx": "./img/filetypes/filetype-doc.png",
                        "xls": "./img/filetypes/filetype-xls.png",
                        "xlsx": "./img/filetypes/filetype-xls.png",
                        "ppt": "./img/filetypes/filetype-ppt.png",
                        "pptx": "./img/filetypes/filetype-ppt.png",
                        "pdf": "./img/filetypes/filetype-pdf.png",
                        "xml": "./img/filetypes/filetype-xml.png",
                        "zip": "./img/filetypes/filetype-zip.png",
                        "msg": "./img/filetypes/filetype-msg.png",
                        "htm": "./img/filetypes/filetype-html.png",
                        "html": "./img/filetypes/filetype-html.png"
                    },
                    "unresolved": "./img/1x1.png"
                }
            }
        },
        "categories": {
            "show": true,
            "exclude": [
                "/^System.*/",
                "/^ModifiedDate.*/",
                "/^Projects (JIRA)$/",
                "/^Author$/",
                "/^GDPR$/",
                "/^Tabs/",
                "/^Type$/",
                "/^Filetype$/i"
            ]
        }
    },
    "details": {
        "show": true,
        "properties": {
            "show": true,
            "exclude": [
                "/\\$id$/",
                "/^abstract$/",
                "/^extracts$/",
                "/^categories$/",
                "/^content$/",
                "/^metaList$/",
                "/^Xtitle$/",
                "/^url$/",
                "/^$/"
            ]
        },
        "metadata": {
            "show": true,
            "exclude": [
                "/^_?IntelliSearch\\./i",
                "/^ItemId(Hash|Uri)$/",
                "/^CrawlerName/",
                "/^CrawledDate/",
                "/^XSystem$/",
                "/^Exists$/"
            ]
        },
        "categories": {
            "show": true,
            "exclude": [
                "/^System.*/",
                "/^ModifiedDate.*/",
                "/^Projects \\(JIRA\\)$/",
                "/^Author$/",
                "/^GDPR$/",
                "/^Tabs/",
                "/^Type$/"
            ]
        }
    },
    "settingsDialogHints": {
        "query": {
            "queryText": ""
        },
        "autocomplete": {
            "enabled": "Enable the autocomplete service?"
        },
        "find": {
            "enabled": "Enable the find service?"
        },
        "categorize": {
            "enabled": "Enable the categorize service?"
        }
    }
}

{
    "baseUrl": "http://searchmanager.demo.intellisearch.no",
    "authentication": {
        "baseUrl": "https://auth.intellisearch.com",
        "basePath": "",
        "servicePath": "auth/login",
        "enabled": false
    },
    "autocomplete": {
        "enabled": true
    },
    "find": {
        "triggers": {
            "queryChangeDelay": -1,
            "queryChangeMinLength": 0
        }
    },
    "categorize": {
        "triggers": {
            "queryChangeDelay": -1,
            "queryChangeMinLength": 0
        }
    },
    "query": {
        "clientId": "plain-sample",
        "matchGenerateContent": true,
        "matchGrouping": true,
        "matchPageSize": 10,
        "searchType": "Keywords",
        "categorizationType": "DocumentHitsOnly"
    }
}

{
    "configuration": {
        "settings": {
            "enabled": false
        },
        "categoryPresentation": {
            "enabled": false
        }
    },
    "match": {
        "pager": {
            "size": 10,
            "addFirst": true,
            "addPrev": true,
            "addNext": true,
            "addLast": true
        },
        "icon": {
            "primary": {
                "property": "sourceName",
                "match": "/(.+)/",
                "replacement": "$1",
                "map": {
                    "docs.global.ints": "images/source-fileshare3.png",
                    "demo-esp": "images/source-fileshare3.png",
                    "Mailbox": "images/source-mail.png"
                },
                "unresolved": "images/source-unresolved.png"
            },
            "overlay": {
                "filetype": {
                    "metadata": "Extension",
                    "match": "/\\.?(.+)/",
                    "replacement": "$1",
                    "map": {
                        "txt": "images/filetype-txt.png",
                        "doc": "images/filetype-doc.png",
                        "docx": "images/filetype-doc.png",
                        "xls": "images/filetype-xls.png",
                        "xlsx": "images/filetype-xls.png",
                        "ppt": "images/filetype-ppt.png",
                        "pptx": "images/filetype-ppt.png",
                        "pdf": "images/filetype-pdf.png",
                        "xml": "images/filetype-xml.png",
                        "zip": "images/filetype-zip.png",
                        "msg": "images/filetype-msg.png",
                        "htm": "images/filetype-html.png",
                        "html": "images/filetype-html.png"
                    },
                    "unresolved": "images/1x1.png"
                }
            }
        },
        "categories": {
            "show": true,
            "exclude": [
                "/^System.*/",
                "/^ModifiedDate.*/",
                "/^Projects (JIRA)$/",
                "/^Author$/",
                "/^GDPR$/",
                "/^Tabs/",
                "/^Type$/",
                "/^Filetype$/i"
            ]
        }
    },
    "details": {
        "show": true,
        "properties": {
            "show": true,
            "exclude": [
                "/\\$id$/",
                "/^abstract$/",
                "/^extracts$/",
                "/^categories$/",
                "/^content$/",
                "/^metaList$/",
                "/^Xtitle$/",
                "/^url$/",
                "/^$/"
            ]
        },
        "metadata": {
            "show": true,
            "exclude": [
                "/^_?IntelliSearch\\./i",
                "/^ItemId(Hash|Uri)$/",
                "/^CrawlerName/",
                "/^CrawledDate/",
                "/^XSystem$/",
                "/^Exists$/"
            ]
        },
        "categories": {
            "show": true,
            "exclude": [
                "/^System.*/",
                "/^ModifiedDate.*/",
                "/^Projects \\(JIRA\\)$/",
                "/^Author$/",
                "/^GDPR$/",
                "/^Tabs/",
                "/^Type$/"
            ]
        }
    },
    "settingsDialogHints": {
        "query": {
            "queryText": ""
        },
        "autocomplete": {
            "enabled": "Enable the autocomplete service?"
        },
        "find": {
            "enabled": "Enable the find service?"
        },
        "categorize": {
            "enabled": "Enable the categorize service?"
        }
    }
}
